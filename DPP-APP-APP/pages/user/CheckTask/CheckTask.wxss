.data-top{
    width: 15vh;
    top: 0%;
    height: 5vh;
    margin: 1vh 0vh 0vh 1vh;
    padding-top: 1vh;
	  text-align: center;
    background-color: #A7D7B9;
    display: inline-block;position: absolute;
}
.onFinish {
  width: 48%;
  left: 1vh;
  bottom: 1vh;
  justify-content: center;
  background: #268dcd;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  position: absolute;
}
.finish {
  width: 48%;
  justify-content: center;
  background: #268dcd;
  right: 1vh;
  bottom: 1vh;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  position: absolute;
}

.list-show {
  top: 80rpx;
  animation: slide 500ms;
}

@keyframes slide {
  0% {
    top: -500rpx;
  }

  100% {
    top: 80rpx;
  }
}
.bg-g{
    background-color:	white;
}
/*-- table--*/
.table {
  position: absolute;
  width: 97%;
  height: 82%;
  margin: 1vh 1vh 1vh 1vh;
  border: 0px solid darkgray;
}
.table-tr {
  width: 100%;
  height: 100%;
}
.tr-h {
    display: flex;
    height: 3rem;
    width: 100%;
    justify-content: center;
    align-items: center;
    background-color: #DCDCDC;
    border-bottom: 1px solid darkgray;
}
.tr {
    display: flex;
    height: 5rem;
    width: 100%;
    justify-content: center;
    align-items: center;
    border-bottom: 1px solid darkgray;
}
.td {
    color: black;
    width:50%;
    justify-content: center;
    text-align: center;
}
.th {
    width: 50%;
    justify-content: center;
    background: #268dcd;
    color: #fff;
    display: flex;
    height: 3rem;
    align-items: center;
}
